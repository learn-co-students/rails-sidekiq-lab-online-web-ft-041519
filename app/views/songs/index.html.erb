<%= form_tag songs_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import Songs" %>
<% end %>
<ul>
<% @songs.each do |song| %>
  <li><%= "#{song.title} #{song.artist_name}" %> - <%= link_to song.title, songs_path(@song)%> </li>

<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>

